    </form>
    <br />
    <button type="button">회원가입</button>
    <div class="result"></div>

    <script>
        const resultBox = document.querySelector('.result');

        function axiosPost() {
            const form = document.forms['formPost']
            const data = {
                id: form.data.id,
                pw: form.data.password
            }
        }
        axios({
            method: 'POST',
            url: '/axios',
            data
        })
    </script>
</body>
